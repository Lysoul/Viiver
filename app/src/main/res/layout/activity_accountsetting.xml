<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_accountsetting"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.thuggerbrain.viiver.activity.AccountSettingActivity"
    android:orientation="vertical">


    <android.support.v7.widget.Toolbar
        android:id="@+id/toobarAccountSetting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary" />
    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.thuggerbrain.viiver.fragment.FragmentUseraccount"
        tools:layout="@layout/fragment_useraccount" />
</LinearLayout>
